@section PageStyles {
  <style>
    .status-badge {
      font-size: 0.75rem;
      padding: 0.35em 0.65em;
      border-radius: 0.25rem;
      display: inline-flex;
      align-items: center;
      text-align: center;
      white-space: nowrap;
      vertical-align: baseline;
    }

    .status-available {
      background-color: rgba(40, 199, 111, 0.1);
      color: #28c76f;
      border: 1px solid rgba(40, 199, 111, 0.2);
    }

    .status-maintenance {
      background-color: rgba(255, 159, 67, 0.1);
      color: #ff9f43;
      border: 1px solid rgba(255, 159, 67, 0.2);
    }

    .crane-card {
      transition: all 0.3s;
    }

    .crane-card:hover {
      transform: translateY(-5px);
      box-shadow: 0 0.25rem 1rem rgba(161, 172, 184, 0.45);
    }

    .maintenance-timer {
      margin-top: 5px;
      font-size: 0.85rem;
    }

    .action-buttons .btn {
      padding: 0.25rem 0.5rem;
      font-size: 0.75rem;
    }

    .action-buttons .btn+.btn {
      margin-left: 0.25rem;
    }

    .crane-image-container {
      position: relative;
      height: 160px;
      background-color: #f5f5f9;
      border-radius: 0.375rem 0.375rem 0 0;
      overflow: hidden;
    }

    .crane-image {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .crane-image-placeholder {
      display: flex;
      align-items: center;
      justify-content: center;
      height: 100%;
      color: #8592a3;
    }

    .upload-image-btn {
      position: absolute;
      bottom: 0.5rem;
      right: 0.5rem;
      width: 2rem;
      height: 2rem;
      border-radius: 50%;
      background-color: rgba(255, 255, 255, 0.9);
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      transition: all 0.2s;
    }

    .upload-image-btn:hover {
      background-color: #fff;
      box-shadow: 0 0.125rem 0.25rem rgba(161, 172, 184, 0.4);
    }
  </style>
}

